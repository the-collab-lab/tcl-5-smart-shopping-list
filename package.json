            "git add"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "cypress": "^13.6.1",
        "typescript": "^3.2.4",
        "react-scripts": "5.0.0"
    }
